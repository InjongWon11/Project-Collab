{
    "name": "Project-Collab",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && npm run build && npm run serve",
        "server": "nodemon server.js",
        "dev":
            "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" ",
        "update":
            "npm install && cd client && npm install"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "express": "^4.16.2",
        "express-session": "^1.15.6",
        "mongodb": "^3.0.5",
        "mongoose": "^5.0.11",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "yarn": "^1.5.1",
        "nodemon": "^1.17.2",
        "concurrently": "^3.5.0"
    }
}
